// About VIEW
"about.title" = "Sobre";
"about.subtitle1" = "SOBRE LA APLICACIÓN “GUARDIANES DE LA SALUD” ";
"about.subtitle2" = "Sobre la participación voluntaria";
"about.subtitle3" = "Agradecimiento";
